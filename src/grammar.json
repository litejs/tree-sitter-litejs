{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "litejs",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_item"
      }
    },
    "_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "js_block"
        },
        {
          "type": "SYMBOL",
          "name": "css_block"
        },
        {
          "type": "SYMBOL",
          "name": "view_directive"
        },
        {
          "type": "SYMBOL",
          "name": "el_directive"
        },
        {
          "type": "SYMBOL",
          "name": "plugin_directive"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "binding_line"
        },
        {
          "type": "SYMBOL",
          "name": "event_line"
        },
        {
          "type": "SYMBOL",
          "name": "raw_text_line"
        },
        {
          "type": "SYMBOL",
          "name": "template_line"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "_space": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "_rest": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "_word": {
      "type": "PATTERN",
      "value": "\\S+"
    },
    "js_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%js"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "block_body"
              },
              {
                "type": "SYMBOL",
                "name": "block_empty_line"
              }
            ]
          }
        }
      ]
    },
    "css_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%css"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "block_body"
              },
              {
                "type": "SYMBOL",
                "name": "block_empty_line"
              }
            ]
          }
        }
      ]
    },
    "view_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%view"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "_word"
            },
            "named": true,
            "value": "view_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_space"
                },
                {
                  "type": "FIELD",
                  "name": "parent",
                  "content": {
                    "type": "ALIAS",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_word"
                    },
                    "named": true,
                    "value": "view_parent"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_rest"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "el_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%el"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "element_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_space"
                },
                {
                  "type": "FIELD",
                  "name": "params",
                  "content": {
                    "type": "ALIAS",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_rest"
                    },
                    "named": true,
                    "value": "element_params"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "element_name": {
      "type": "PATTERN",
      "value": "[A-Za-z][A-Za-z0-9_-]*"
    },
    "plugin_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "plugin_keyword"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "_space"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "SYMBOL",
                  "name": "_rest"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "plugin_keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "%def"
        },
        {
          "type": "STRING",
          "value": "%slot"
        },
        {
          "type": "STRING",
          "value": "%each"
        },
        {
          "type": "STRING",
          "value": "%svg"
        },
        {
          "type": "STRING",
          "value": "%start"
        },
        {
          "type": "STRING",
          "value": "%child"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "STRING",
              "value": "/"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_rest"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "binding_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "binding"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_rest"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "binding": {
      "type": "PATTERN",
      "value": ";[a-zA-Z$]\\w*[!:]?"
    },
    "event_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "event"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_rest"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "event": {
      "type": "PATTERN",
      "value": "@[a-zA-Z]\\w*[!:]?"
    },
    "raw_text_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "= "
            },
            {
              "type": "STRING",
              "value": "\\ "
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_rest"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "template_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "selector"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "_space"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "binding"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "event"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "text_content"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "selector": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_selector_part"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_space"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "child_combinator"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_space"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "_selector_part"
              }
            ]
          }
        }
      ]
    },
    "_selector_part": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "tag_name"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "id_selector"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "class_selector"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "attribute_selector"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "id_selector"
              },
              {
                "type": "SYMBOL",
                "name": "class_selector"
              },
              {
                "type": "SYMBOL",
                "name": "attribute_selector"
              }
            ]
          }
        }
      ]
    },
    "tag_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9-]*"
    },
    "id_selector": {
      "type": "PATTERN",
      "value": "#[A-Za-z_-][A-Za-z0-9_-]*"
    },
    "class_selector": {
      "type": "PATTERN",
      "value": "\\.[A-Za-z_-][A-Za-z0-9_-]*"
    },
    "attribute_selector": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "PATTERN",
          "value": "[^\\]\\n]*"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "child_combinator": {
      "type": "STRING",
      "value": ">"
    },
    "text_content": {
      "type": "PATTERN",
      "value": "[^;@\\n]+"
    }
  },
  "extras": [],
  "conflicts": [
    [
      "selector"
    ]
  ],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "block_body"
    },
    {
      "type": "SYMBOL",
      "name": "block_empty_line"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}