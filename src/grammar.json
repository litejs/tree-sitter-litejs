{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "litejs",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_item"
      }
    },
    "_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "js_block"
        },
        {
          "type": "SYMBOL",
          "name": "css_block"
        },
        {
          "type": "SYMBOL",
          "name": "plugin_line"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "template_line"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "_space": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "_rest": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "js_block": {
      "type": "PREC_DYNAMIC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "%js"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_space"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "\\n"
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "block_body"
                },
                {
                  "type": "SYMBOL",
                  "name": "block_empty_line"
                }
              ]
            }
          }
        ]
      }
    },
    "css_block": {
      "type": "PREC_DYNAMIC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "%css"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_space"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "\\n"
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "block_body"
                },
                {
                  "type": "SYMBOL",
                  "name": "block_empty_line"
                }
              ]
            }
          }
        ]
      }
    },
    "plugin_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "plugin_name"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "STRING",
                "value": "%js"
              },
              "named": true,
              "value": "plugin_name"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "STRING",
                "value": "%css"
              },
              "named": true,
              "value": "plugin_name"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "_space"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "SYMBOL",
                  "name": "_rest"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "plugin_name": {
      "type": "PATTERN",
      "value": "%[a-z]+"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "STRING",
              "value": "/"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_rest"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "template_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "selector"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "_space"
                        },
                        {
                          "type": "CHOICE",
                          "members": [
                            {
                              "type": "SYMBOL",
                              "name": "_binding_list"
                            },
                            {
                              "type": "ALIAS",
                              "content": {
                                "type": "SYMBOL",
                                "name": "_rest"
                              },
                              "named": true,
                              "value": "text_content"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "_binding_list"
            },
            {
              "type": "SYMBOL",
              "name": "raw_text"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "_binding_list": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "binding"
              },
              {
                "type": "SYMBOL",
                "name": "event"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "binding_args"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "selector": {
      "type": "PATTERN",
      "value": "([^ \\t\\n;@\\[]|\\[[^\\]\\n]*\\])+([ \\t]*>[ \\t]*([^ \\t\\n;@\\[]|\\[[^\\]\\n]*\\])+)*"
    },
    "binding": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": ";[a-zA-Z$]\\w*[!:]?"
        }
      }
    },
    "event": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "@[a-zA-Z]\\w*[!:]?"
        }
      }
    },
    "binding_args": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^;@\\n]+"
        }
      }
    },
    "raw_text": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "= "
            },
            {
              "type": "STRING",
              "value": "\\ "
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "_rest"
              },
              "named": true,
              "value": "text_content"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "block_body"
    },
    {
      "type": "SYMBOL",
      "name": "block_empty_line"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}