{
  "name": "tree-sitter-litejs",
  "version": "26.2.1",
  "description": "Tree-sitter grammar for LiteJS UI templates",
  "license": "MIT",
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "tree-sitter",
    "litejs"
  ],
  "scripts": {
    "test": "npx tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.litejs",
      "file-types": [
        "ui"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/litejs/tree-sitter-litejs.git"
  },
  "dependencies": {
    "nan": "2.25.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "0.26.5"
  }
}
